{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "serve": "vite preview",
    "dev": "vite --open",
    "build": "vue-tsc --noEmit && vite build && npm run crush-images && npm run copy:abi",
    "lint": "eslint --ext .js,.vue,.ts src",
    "start": "npm run dev",
    "crush-images": "node scripts/crush-images",
    "copy:abi": "copyfiles -f \"../build/contracts/*.json\" ./dist/abi",
    "format": "prettier .  --write"
  },
  "dependencies": {
    "@metamask/onboarding": "^1.0.1",
    "@walletconnect/web3-provider": "^1.7.1",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "bootstrap-vue-dialog": "^0.1.0",
    "ethereumjs-util": "7.1.4",
    "isomorphic-ws": "^4.0.1",
    "node-sass": "^6.0.1",
    "seedrandom": "^3.0.5",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-observe-visibility": "^1.0.0",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.1",
    "vue-spinner": "^1.0.4",
    "vuex": "^3.6.2",
    "vuex-cache": "^3.4.0",
    "web3": "^1.3.4",
    "web3-eth-contract": "^1.3.4",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@types/lodash": "^4.14.168",
    "@types/seedrandom": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@vue/eslint-config-typescript": "^5.0.2",
    "copyfiles": "^2.4.1",
    "eslint": "8",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "8",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "sass": "^1.49.7",
    "typescript": "~4.1.5",
    "vite": "2.7.9",
    "vite-plugin-vue2": "1.9.3",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.31.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "resolutions": {
    "ethereumjs-util": "7.1.4"
  }
}
